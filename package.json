{
  "name": "@sdp.nd/rc-calendar",
  "version": "2.0.0",
  "description": "React Calendar",
  "keywords": [
    "react",
    "react-calendar",
    "react-component",
    "calendar",
    "ui component",
    "ui",
    "component"
  ],
  "files": [
    "lib",
    "assets/*.css",
    "dist"
  ],
  "main": "lib/index",
  "entry": {
    "rc-calendar": [
      "./assets/index.less",
      "./src/index.js"
    ]
  },
  "homepage": "http://github.com/react-component/calendar",
  "author": "yiminghe@gmail.com",
  "bugs": {
    "url": "http://github.com/react-component/calendar/issues"
  },
  "licenses": "MIT",
  "config": {
    "port": 8002
  },
  "scripts": {
    "lint": "stylelint \"src/**/*.css\" & stylelint \"src/**/*.scss\" & stylelint \"src/**/*.less\" & eslint --ext .js,.jsx src",
    "fix": "stylelint --fix \"src/**/*.css\" & stylelint --fix \"src/**/*.scss\" & stylelint --fix \"src/**/*.less\" & eslint --ext .js,.jsx --fix src",
    "build": "rc-tools run compile",
    "gh-pages": "rc-tools run gh-pages",
    "start": "rc-tools run server",
    "pub": "rc-tools run pub --babel-runtime",
    "karma": "rc-tools run karma",
    "saucelabs": "rc-tools run saucelabs",
    "test": "jest",
    "chrome-test": "rc-tools run chrome-test",
    "coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*"
    ]
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "pre-commit": "1.x",
    "@sdp.nd/eslint-config-react": "^2.0.0",
    "eslint": "^4.14.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-promise": "^3.5.0",
    "stylelint": "^8.4.0",
    "stylelint-config-standard": "^18.0.0",
    "async": "~0.9.0",
    "bootstrap": "~3.3.2",
    "coveralls": "^2.11.15",
    "expect.js": "~0.3.1",
    "jest": "^18.1.0",
    "jquery": "~1.11.3",
    "object-assign": "4.x",
    "rc-dialog": "^6.0.1",
    "rc-select": "^6.0.6",
    "rc-time-picker": "2.x",
    "rc-tools": "5.x",
    "react": "15.x",
    "react-addons-test-utils": "15.x",
    "react-dom": "15.x"
  },
  "dependencies": {
    "@sdp.nd/nd-input": "^1.0.0",
    "babel-runtime": "6.x",
    "classnames": "2.x",
    "moment": "2.x",
    "rc-trigger": "1.x",
    "rc-util": "^4.0.1"
  }
}
